<h1>Productors#new</h1>
<%= form_with model: @productor, local:true do |f| %>
<%= f.label :name, "Nom du producteur" %>
<%= f.text_field :name %>
<%= f.label :description, "Description" %>
<%= f.text_area :description %>
<%= f.label :phone_number, "Numéro de téléphone" %>
<%= f.text_field :phone_number %>
<%= f.submit "Créer le producteur", class: "btn btn-primary" %>
<% end %>